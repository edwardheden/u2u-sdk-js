{
    "name": "@hashgraph/sdk",
    "version": "2.0.0-pre",
    "description": "Hederaâ„¢ Hashgraph SDK",
    "main": "lib/index.js",
    "browser": "src/browser.js",
    "license": "Apache-2.0",
    "author": "Launchbadge <contact@launchbadge.com>",
    "repository": "https://github.com/hashgraph/hedera-sdk-js",
    "bugs": "https://github.com/hashgraph/hedera-sdk-js/issues",
    "type": "module",
    "keywords": [
        "hedera",
        "hashgraph",
        "sdk",
        "transactions"
    ],
    "files": [
        "lib/",
        "src/"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "browserslist": [
        "last 1 Chrome versions",
        "last 1 Firefox versions"
    ],
    "scripts": {
        "docs": "typedoc --theme minimal --mode modules --excludeNotExported --excludePrivate --excludeProtected --out docs --listInvalidSymbolLinks",
        "compile:js": "babel src -d lib",
        "lint": "run-s lint:*",
        "lint:types": "tsc",
        "lint:format": "prettier src test '*.json' 'src/*.js' --check",
        "lint:js": "eslint --fix 'src/**/*.js' 'test/**/*.js'",
        "format": "prettier src test '*.json' 'src/*.js' --write",
        "prepare": "run-s compile:js lint:types",
        "test": "run-s test:node test:browser:chrome test:browser:firefox",
        "test:node": "mocha -r @babel/register -r chai/register-expect 'test/unit/**/*.js' 'test/integration/**/*.js'",
        "test:browser:chrome": "vite serve --port 9001 test/ & sleep 2; mocha-webdriver-runner --headless-chrome http://localhost:9001/; kill %1",
        "test:browser:firefox": "vite serve --port 9002 test/ & sleep 2; mocha-webdriver-runner --headless-firefox http://localhost:9002/; kill %1"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.1.7",
        "@hashgraph/cryptography": "^1.0.2",
        "@hashgraph/proto": "^1.0.2",
        "@improbable-eng/grpc-web": "^0.13.0",
        "@stablelib/sha384": "^1.0.0",
        "bignumber.js": "^9.0.1",
        "js-sha3": "^0.8.0",
        "long": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.10.4",
        "@babel/register": "^7.11.5",
        "@types/chai": "^4.2.12",
        "@types/long": "^4.0.1",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "chai": "^4.2.0",
        "chromedriver": "^85.0.1",
        "eslint": "^7.10.0",
        "eslint-plugin-chai-expect": "^2.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.6.3",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-node": "^11.1.0",
        "geckodriver": "^1.20.0",
        "lerna": "^3.22.1",
        "mocha": "^8.1.3",
        "mocha-webdriver-runner": "^0.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "typescript": "^4.0.3",
        "vite": "^1.0.0-rc.4"
    }
}
