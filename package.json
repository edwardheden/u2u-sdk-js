{
    "name": "@hashgraph/sdk",
    "version": "2.0.0-pre",
    "description": "Hederaâ„¢ Hashgraph SDK for JavaScript with support for Node.js 10+ and modern browsers.",
    "homepage": "https://docs.hedera.com/docs/hedera-sdks",
    "repository": "https://github.com/hashgraph/hedera-sdk-js",
    "bugs": "https://github.com/hashgraph/hedera-sdk-js/issues",
    "author": "Launchbadge <contact@launchbadge.com>",
    "license": "Apache-2.0",
    "keywords": [
        "hedera",
        "hashgraph",
        "sdk"
    ],
    "engines": {
        "node": ">=0.10.20.1"
    },
    "main": "lib/index.js",
    "module": "src/index.js",
    "scripts": {
        "docs": "typedoc --theme minimal --mode modules --excludeNotExported --excludePrivate --excludeProtected --out docs --listInvalidSymbolLinks",
        "compile": "babel src -d lib",
        "check:types": "tsc",
        "check:format": "prettier src test '*.json' '*.js' --check",
        "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
        "format": "prettier src test '*.json' '*.js' --write",
        "build": "run-s check:types lint check:format compile",
        "test": "run-s test:node test:browser:chrome test:browser:firefox",
        "test:node": "mocha -r @babel/register -r chai/register-expect 'test/unit/**/*.js'",
        "test:browser:chrome": "parcel serve --dist-dir .parcel-cache/dist --no-source-maps --no-autoinstall --log-level=none --no-hmr -p 9001 test/index.html & sleep 2; mocha-webdriver-runner --headless-chrome http://localhost:9001/; kill %1",
        "test:browser:firefox": "parcel serve --dist-dir .parcel-cache/dist --no-source-maps --no-autoinstall --log-level=none --no-hmr -p 9002 test/index.html & sleep 2; mocha-webdriver-runner --headless-firefox http://localhost:9002/; kill %1"
    },
    "dependencies": {
        "tweetnacl": "^1.0.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/register": "^7.9.0",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.13.4",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-chai-expect": "^2.1.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsdoc": "^24.0.1",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-node": "^11.1.0",
        "mocha": "^7.1.2",
        "mocha-webdriver-runner": "^0.6.2",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.0.0-nightly.248",
        "prettier": "^2.0.5",
        "typedoc": "^0.17.6",
        "typescript": "^3.8.3"
    }
}
